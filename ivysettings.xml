<?xml version="1.0" encoding="ISO-8859-1"?>
	<ivysettings>
	    <include file="${basedir}/${bwda.importer.dir}/ivysettings.xml"/>
	    <resolvers>
	        <chain name="shared">
	            <filesystem name="snapshots" m2compatible="true">
	                <artifact pattern="${bwda.repo.dir}/snapshots/[organisation]/[module]/[revision]/[artifact](-[classifier])-[revision].[ext]" />
	                <ivy pattern="${bwda.repo.dir}/snapshots/[organisation]/[module]/[revision]/[artifact](-[classifier])-[revision].[ext]" />
	            </filesystem>
	            <filesystem name="releases" m2compatible="true">
	                <artifact pattern="${bwda.repo.dir}/releases/[organisation]/[module]/[revision]/[artifact](-[classifier])-[revision].[ext]" />
	                <ivy pattern="${bwda.repo.dir}/releases/[organisation]/[module]/[revision]/[artifact](-[classifier])-[revision].[ext]" />
	            </filesystem>
	        </chain>
	    </resolvers>
	    <include url="${ivy.default.settings.dir}/ivysettings-main-chain.xml"/>
	    <resolvers>
	        <chain name="default.${bwda.ivy.settings.id}" returnFirst="true">
	            <resolver ref="local"/>
	            <resolver ref="main"/>
	        </chain>
	    </resolvers>
	    <modules>
	        <module organisation="${ivy.organisation}" name="*" resolver="shared"/>
	    </modules>
	</ivysettings>