<?xml version="1.0" encoding="ISO-8859-1"?>
<ivysettings>
    <include file="${basedir}/${bwda.importer.dir}/ivysettings.xml"/>
    <resolvers>
        <chain name="shared">
            <url name="releases" m2compatible="true">
                <artifact pattern="${wr.repo.basepath}/rel/${wr.repo.artifact.pattern}" />
                <ivy pattern="${wr.repo.basepath}/rel/${wr.repo.ivy.pattern}" />
            </url>
            <url name="snapshots" m2compatible="true">
                <artifact pattern="${wr.repo.basepath}/snaps/${wr.repo.artifact.pattern}" />
                <ivy pattern="${wr.repo.basepath}/snaps/${wr.repo.ivy.pattern}" />
            </url>
        </chain>
    </resolvers>
    <include url="${ivy.default.settings.dir}/ivysettings-main-chain.xml"/>
    <resolvers>
        <chain name="default.${bwda.ivy.settings.id}" returnFirst="true">
            <resolver ref="local"/>
            <resolver ref="main"/>
        </chain>
    </resolvers>
    <modules>
        <module organisation="${ivy.organisation}" name="*" resolver="shared"/>
    </modules>
</ivysettings>