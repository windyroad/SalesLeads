<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://windyroad.org/bwunit-asserts-0.0.0.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>test/UnitTestSuite/steps/The sales leads DB will have a SalesLeads table with the following columns.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="column" minOccurs="0" maxOccurs="unbounded">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="name" type="xsd:string"/>
                                <xsd:element name="type" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>712</pd:endX>
    <pd:endY>70</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1413427508438</pd:targetNamespace>
    <pd:activity name="Describe table">
        <pd:type>com.tibco.plugin.jdbc.JDBCGeneralActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCGeneralActivity</pd:resourceType>
        <pd:x>273</pd:x>
        <pd:y>65</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <jdbcSharedConfig>/resources/SalesLeadDatabase.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcGeneralActivityInput>
                <statement>show columns from salesleads</statement>
            </jdbcGeneralActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="AssertExists">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>405</pd:x>
        <pd:y>65</pd:y>
        <config>
            <processName>/BWUnit/Public/Asserts/AssertExists.process</processName>
        </config>
        <pd:inputBindings>
            <ns:exists-input>
                <ns:content>
                    <xsl:copy-of select="$Describe-table/jdbcGeneralActivityOutput/unknownResultset/row"/>
                </ns:content>
            </ns:exists-input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="AssertEqual">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>571</pd:x>
        <pd:y>67</pd:y>
        <config>
            <processName>/BWUnit/Public/Asserts/AssertEqual.process</processName>
        </config>
        <pd:inputBindings>
            <ns:equal-input>
                <ns:expected>
                    <input>
                        <xsl:for-each select="$Start/input/column">
                            <xsl:sort select="name"/>
                            <column>
                                <name>
                                    <xsl:value-of select="name"/>
                                </name>
                                <type>
                                    <xsl:value-of select="type"/>
                                </type>
                            </column>
                        </xsl:for-each>
                    </input>
                </ns:expected>
                <ns:actual>
                    <input>
                        <xsl:for-each select="$Describe-table/jdbcGeneralActivityOutput/unknownResultset/row">
                            <xsl:sort select="column[name='COLUMN_NAME']/value"/>
                            <column>
                                <name>
                                    <xsl:value-of select="column[name='COLUMN_NAME']/value"/>
                                </name>
                                <type>
                                    <xsl:value-of select="column[name='TYPE']/value"/>
                                </type>
                            </column>
                        </xsl:for-each>
                    </input>
                </ns:actual>
            </ns:equal-input>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Describe table</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Describe table</pd:from>
        <pd:to>AssertExists</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>AssertExists</pd:from>
        <pd:to>AssertEqual</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>AssertEqual</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>