<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>test/UnitTestSuite/steps/The binary version is set.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType/>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <version>
            <xsl:value-of select="($GetSystemProperty/property/@value | $Set-System-Property/property/@newValue)[1]"/>
        </version>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="version" type="xsd:string"/>
    </pd:endType>
    <pd:endX>604</pd:endX>
    <pd:endY>63</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1414549599837</pd:targetNamespace>
    <pd:activity name="Set System Property">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>452</pd:x>
        <pd:y>157</pd:y>
        <config>
            <processName>/util/SetSystemProperty.process</processName>
        </config>
        <pd:inputBindings>
            <property>
                <xsl:attribute name="key">
                    <xsl:value-of select="$GetSystemProperty/property/@key"/>
                </xsl:attribute>
                <xsl:attribute name="value">2.3</xsl:attribute>
            </property>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="GetSystemProperty">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>265</pd:x>
        <pd:y>66</pd:y>
        <config>
            <processName>/util/GetSystemProperty.process</processName>
        </config>
        <pd:inputBindings>
            <property>
                <xsl:attribute name="key">
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/CONST/BINARY_VERSION_PROPERTY"/>
                </xsl:attribute>
            </property>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Set System Property</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetSystemProperty</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetSystemProperty</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>exists($GetSystemProperty/property/@value)</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>GetSystemProperty</pd:from>
        <pd:to>Set System Property</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>