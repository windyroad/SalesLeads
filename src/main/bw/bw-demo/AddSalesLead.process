<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://windyroad.com.au/schemas/SalesLead.xsd">
    <xsd:import namespace="http://windyroad.com.au/schemas/SalesLead.xsd" schemaLocation="/SalesLead.xsd"/>
    <pd:name>AddSalesLead.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:SalesLead"/>
    <pd:startX>150</pd:startX>
    <pd:startY>65</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>450</pd:endX>
    <pd:endY>64</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1413194211051</pd:targetNamespace>
    <pd:activity name="JDBC Update">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>294</pd:x>
        <pd:y>68</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>true</emptyStrAsNil>
            <jdbcSharedConfig>/resources/SalesLeadDatabase.sharedjdbc</jdbcSharedConfig>
            <statement>INSERT INTO SalesLeads (FirstName, LastName, Email, PhoneNumber) VALUES (?, ?, ?, ?)</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>FirstName</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>LastName</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>Email</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PhoneNumber</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <FirstName>
                    <xsl:value-of select="$Start/pfx:SalesLead/pfx:FirstName"/>
                </FirstName>
                <LastName>
                    <xsl:value-of select="$Start/pfx:SalesLead/pfx:LastName"/>
                </LastName>
                <Email>
                    <xsl:value-of select="$Start/pfx:SalesLead/pfx:Email"/>
                </Email>
                <PhoneNumber>
                    <xsl:value-of select="$Start/pfx:SalesLead/pfx:PhoneNumber"/>
                </PhoneNumber>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>JDBC Update</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Update</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>